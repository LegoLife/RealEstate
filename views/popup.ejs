<h1><%=mod.Street%> <%=mod.City%>,<%=mod.State%> <%=mod.Zip%></h1>

<div class="card" style="width:50rem; border-radius:40px;">

    <div id="carouselExampleControls<%=mod.id%>" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner" role="listbox">
            <div class="carousel-item active">
                <img class="d-block img-fluid" src="<%=mod.PhotoPath%>" style="width:auto; max-height:500px;" />
            </div>
            <div class="carousel-item">
                <img class="d-block img-fluid" src="https://pixabay.com/get/ea34b40b2ef7043ed95c4518b7444690eb7fe6d304b014409df8c57aa4e5b1_640.jpg" style="width:auto; max-height:500px;">
            </div>
            <% mod.Photos.forEach(function(item) { %>

            <div class="carousel-item">
                <img class="d-block img-fluid" src="<%=item%>" style="width:auto; max-height:500px;" />
            </div>
            <% }); %>

        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls<%=mod.id%>" role="button" data-slide="prev">
            <span  class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span  class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControls<%=mod.id%>" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>

    <div class="card-block ">
        <h4 class="card-title">Price: $<%=mod.Price%></h4>
        Seller:<%=mod.Owner%><br />
        Posted: <%=mod.DatePosted%><br />
        Bath Rooms:<%=mod.BathRooms%><br />
        Square Footage:<%=mod.SquareFt%>
    </div>
    <div class="card-block">
        <p class="card-text"><%=mod.AboutLong%></p>
    </div>
    <ul class="list-group list-group-flush">
        <li class="list-group-item">Cras justo odio</li>
        <li class="list-group-item">Dapibus ac facilisis in</li>
        <li class="list-group-item">Vestibulum at eros</li>
    </ul>
</div>

